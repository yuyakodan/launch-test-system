{
  "name": "launch-test-system",
  "version": "0.1.0",
  "description": "Miyabi自律型開発フレームワークで構築されたプロジェクト",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:local": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:create": "wrangler d1 create launch-test-db",
    "db:migrations": "wrangler d1 migrations apply launch-test-db",
    "db:studio": "wrangler d1 studio launch-test-db",
    "r2:create": "wrangler r2 bucket create launch-test-storage",
    "queue:create": "wrangler queues create launch-test-tasks"
  },
  "keywords": [
    "miyabi",
    "autonomous",
    "ai-agent",
    "cloudflare-workers"
  ],
  "author": "",
  "dependencies": {
    "hono": "^4.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.60.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
